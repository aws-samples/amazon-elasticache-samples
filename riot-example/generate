#!/bin/bash

source env.sh

cmd_to_run="./bin/riot generate \
${GLOBAL_DRY_RUN} \
${GLOBAL_DEBUG} \
${GLOBAL_PROGRESS} \
${GLOBAL_LETTUCE_LOG_LEVEL} \
${GENERATE_KEY_PREFIX} \
${GENERATE_TTL_EXPIRATION} \
${GENERATE_CLUSTER} \
${GLOBAL_SLEEP_MS} \
${GENERATE_INSECURE_TLS} \
${GENERATE_RESP_VERSION} \
${GENERATE_TIMEOUT} \
${GENERATE_SIZE_KEY} \
${GENERATE_THREADS} \
${GENERATE_CLIENT_NAME} \
${GENERATE_KEY_COUNT} \
${GENERATE_LOG_FILE} \
${GENERATE_POOL} \
${GENERATE_BATCH_COUNT} \
${GENERATE_URI} \
${GENERATE_DATA_TYPE}"

echo
echo "Running command:"
echo
echo "$cmd_to_run"
eval "$cmd_to_run"

